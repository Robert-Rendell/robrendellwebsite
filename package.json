{
  "name": "robrendellwebsite",
  "version": "1.0.0",
  "description": "nodejs backend api for robrendellwebsite",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc",
    "postinstall": "npm run build",
    "dev:slow": "ts-node index.ts",
