{
  "name": "robrendellwebsite",
  "version": "1.0.0",
  "description": "nodejs backend api for robrendellwebsite",
  "main": "index.js",
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "start": "node dist/index.js",
    "build": "tsc && npm run add-cv-to-build",
    "postinstall": "npm run build",
    "dev:slow": "ts-node index.ts",
    "dev": "ts-node-dev --respawn --pretty --transpile-only index.ts",
    "test": "jest",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "format": "prettier -w ./src",
    "prepare": "husky install",
    "update-common": "npm install robrendellwebsite-common",
    "husky-add-hook": "husky add .husky/pre-commit \"npm run build\"",
    "add-cv-to-build": "cp ./src/resources/CV_Robert_Rendell.pdf ./dist/src/resources/CV_Robert_Rendell.pdf",
    "parse-keep-notes:insects": "ts-node ./src/scripts/parse-google-keep-takeout.ts Insects",
    "parse-keep-notes:lichen": "ts-node ./src/scripts/parse-google-keep-takeout.ts Lichen",
    "parse-keep-notes:flowers": "ts-node ./src/scripts/parse-google-keep-takeout.ts AAA_Flowers wild-flowers",
    "parse-keep-notes:fungi": "ts-node ./src/scripts/parse-google-keep-takeout.ts Fungi",
    "parse-keep-notes:arachnids": "ts-node ./src/scripts/parse-google-keep-takeout.ts Arachnids",
    "parse-all-keep-notes": "npm run parse-keep-notes:flowers && npm run parse-keep-notes:fungi && npm run parse-keep-notes:arachnids && npm run parse-keep-notes:lichen"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Robert-Rendell/robrendellwebsite.git"
  },
  "author": "Robert Rendell",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Robert-Rendell/robrendellwebsite/issues"
  },
  "homepage": "https://github.com/Robert-Rendell/robrendellwebsite#readme",
  "devDependencies": {
    "@babel/core": "^7.16.0",
    "@babel/preset-env": "^7.16.0",
    "@babel/preset-typescript": "^7.16.0",
    "@types/cors": "~2.8.12",
    "@types/express": "~4.17.13",
    "@types/express-rate-limit": "^5.1.3",
    "@types/jest": "^27.0.2",
    "@types/lodash": "^4.14.177",
    "@types/morgan": "~1.9.3",
    "@types/node": "~14.17.32",
    "@types/nodemailer": "^6.4.7",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~5.2.0",
    "@typescript-eslint/parser": "~5.2.0",
    "babel-jest": "^27.3.1",
    "eslint": "~7.32.0",
    "eslint-config-airbnb-base": "~14.2.1",
    "eslint-plugin-import": "~2.25.2",
    "husky": "^8.0.2",
    "jest": "^27.3.1",
    "prettier": "^2.8.0",
    "ts-node-dev": "~1.1.8",
    "typescript": "~4.4.4"
  },
  "dependencies": {
    "aws-sdk": "~2.1018.0",
    "axios": "~1.0.0",
    "cors": "~2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.17.3",
    "express-rate-limit": "^5.5.1",
    "fast-geoip": "^1.1.88",
    "googleapis": "^108.0.1",
    "helmet": "~4.6.0",
    "lodash": "^4.17.21",
    "morgan": "~1.10.0",
    "nodemailer": "^6.9.0",
    "robrendellwebsite-common": "^1.22.4",
    "ts-node": "~10.4.0",
    "uuid": "^8.3.2"
  }
}
