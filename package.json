  "devDependencies": {
    "@babel/core": "^7.19.6",
    "@babel/preset-env": "^7.19.4",
    "@babel/preset-typescript": "^7.18.6",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.14",
    "@types/jest": "^27.0.2",
    "@types/morgan": "~1.9.3",
    "@types/node": "^18.11.8",
    "@types/uuid": "^8.3.1",
    "@typescript-eslint/eslint-plugin": "~5.2.0",
    "@typescript-eslint/parser": "~5.2.0",
    "babel-jest": "^27.3.1",
    "googleapis": "^108.0.1",
    "husky": "^8.0.1",
    "jest": "^27.3.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4"
  },
  "dependencies": {
    "aws-sdk": "^2.1243.0",
    "cors": "^2.8.5",
    "dotenv": "~10.0.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "helmet": "^6.0.0",
    "morgan": "~1.10.0",
    "robrendellwebsite-common": "^1.0.2",
    "uuid": "^8.3.2"
  }
